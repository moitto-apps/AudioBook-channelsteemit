=begin cell

=object image: image-url="${image-url}", style=img_cover

=object section: section=title, style=section_title
=begin title: id=title
${title}
=end title

=object label: type=date, format=ago, source-format="yyyy-MM-dd'T'HH:mm:ss", source-timezone=GMT, \
                text="${created-at}", style=label_date

=end cell
